{
  "name": "carousel-js",
  "version": "3.1.0",
  "description": "Easily implement a dynamic carousel using minimal javascript. Supports Meteor, AngularJS, React, Polymer and any CSS library, e.g. Bootstrap. ",
  "repository": {
    "type": "git",
    "url": "git://github.com/mkay581/carousel-js.git"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "author": {
    "name": "Mark Kennedy",
    "url": "http://heyimmark.com"
  },
  "license": "MIT",
  "keywords": [
    "carousel",
    "carousels",
    "vanilla carousel",
    "es6 carousel",
    "carousel component",
    "carousel module",
    "react carousel",
    "angular carousel"
  ],
  "main": "./src/carousel.js",
  "dependencies": {
    "lodash.defer": "^4.1.0",
    "module-js": "^5.10.9",
    "promise": "^7.1.1",
    "resource-manager-js": "^2.3.7"
  },
  "devDependencies": {
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.18.0",
    "babel-plugin-transform-object-assign": "^6.8.0",
    "babel-preset-es2015": "^6.18.0",
    "babelify": "^7.3.0",
    "build-tools": "^6.0.4",
    "sinon": "^1.17.3"
  },
  "browserify": {
    "transform": [
      [
        "babelify",
        {
          "presets": [
            "es2015"
          ],
          "plugins": [
            [
              "add-module-exports"
            ],
            [
              "transform-object-assign"
            ],
            [
              "transform-es2015-modules-commonjs"
            ]
          ]
        }
      ]
    ]
  },
  "scripts": {
    "test": "bt test",
    "start": "bt server local"
  }
}
